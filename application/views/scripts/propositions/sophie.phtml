	    function getParamGen(){
		    //récupère les identifiants d'affects sélectionnés
		    var idPg = paramsSelect["G"].marker.attr("idParam");
		    var idPd = paramsSelect["D"].marker.attr("idParam");
		    //récupère les affects
		    var affG = paramsEle[idPg].titre;
		    var affD = paramsEle[idPd].titre;
		    //récupère les pourcentages
		    var prct = getPrctGrad();
		    //construction des paramètres
		    var p = {'json':1,'pro':'propositions/sophie'};
		    p[affG] = prct.a; 
		    p[affD] = prct.b; 
	    	return p; 
	    }
		function envoiTweet() {
			$.post("../api/savetext"
			, getParamsSave(),
			 function(data){
				idPoeme=data.idP;
				var poeme = $('#txtGene').text();
