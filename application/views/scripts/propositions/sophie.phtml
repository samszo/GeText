	    function animeSelect(evt){

			//récupère le paramètre de donnée
		    var e = evt.currentTarget;
			var idP = e.getAttribute("idParam");
			
			//récupère le chemin d'animation
	    	var dir="D";
	    	if(paramsEle[idP]["btnG"]==e.id) dir="G";

	    	//spécifie la sélection
			setSelect(idP, dir)

			//effectue la transition sur le chemin
	    	transitionMarker(paramsSelect[dir].marker, paramsSelect[dir].path);
	    	
	    }	    

	    function setSelect(idP, dir){
	    	paramsSelect[dir].path = svgChoix.select("#"+paramsEle[idP]["lien"+dir]);
	    	paramsSelect[dir].path.attr("idParam", idP); 
			//enregistre l'ancien marker s'il existe
	    	if(paramsSelect[dir].marker) paramsSelect[dir].markerOld = paramsSelect[dir].marker;
			//récupère le point de départ
	    	var startPoint = pathStartPoint(paramsSelect[dir].path);
			//construction du marker
			var marker = svgChoix.append("circle");
	    	marker.attr("r", 6)
	    	    .attr("idParam", idP)
	    	    .attr("dir", dir)
	    	    .attr("style", "fill:"+paramsEle[idP].couleur)
	    	    .attr("transform", "translate(" + startPoint + ")");
	    	paramsSelect[dir].marker = marker; 
	    }
	    
  	  	//Get path start point for placing marker
		function pathStartPoint(path) {
  	    	var d = path.attr("d"),
  	    	dsplitted = d.split(" ");
  	    	return dsplitted[1].split(",");
  	  	}

  	  	function transitionMarker(marker, path) {
  	    	marker.transition()
  	        	.duration(6000)
  	        	.attrTween("transform", translateAlong(path.node()))
  	        	.each("end", endSelect);// infinite loop
  	  	}
  	  	function translateAlong(path) {
  	    	var l = path.getTotalLength();
      		return function(i) {
        		return function(t) {
	          		var p = path.getPointAtLength(t * l);
    	      		return "translate(" + p.x + "," + p.y + ")";//Move marker
        		}
      		}
  	  	}	    	
	    function endSelect(){
		    //récupère le paramètre de donnée
	    	var idP = this.getAttribute("idParam");
			var dir = this.getAttribute("dir");
	    	//masque les boutons de selection
			paramsEle[idP].eSlt.setAttribute("visibility","hidden");	    		    	
			paramsEle[idP].eInfo.setAttribute("visibility","hidden");
			paramsSelect[dir].txt.text(paramsEle[idP].titre)
					.attr("idParam",idP)
	    	    	.style("fill", paramsEle[idP].couleur)
